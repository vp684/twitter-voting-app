{
  "name": "voting-app",
  "version": "1.0.0",
  "description": "voting app using react and express",
  "main": "server.js",
  "engines": {
    "node": "11.10.0",
    "npm": "6.7.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "server": "nodemon server.js",
