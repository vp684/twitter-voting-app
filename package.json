    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.1",
    "mongodb": "3.1.13",
    "mongoose": "^5.4.19",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-twitter-token": "^1.3.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "nodemon": "^1.18.10"
